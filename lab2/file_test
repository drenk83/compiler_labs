$A = 2x + 1  # Присваивание с опущенным *, NUMBER VAR
print $A     # Вывод полинома
$B = (x + 1)(x + 2)  # Опущенное * между скобками
print $B             # Должен вывести x^2 + 3x + 2
$C = $A + $B  # Сложение полиномов
print $C      # x^2 + 5x + 3
$D = $C - x^2  # Вычитание, VAR POW NUMBER
print $D       # 5x + 3
$E = 3(x^3 + 2)  # Опущенное * NUMBER LPAREN, ^ в VAR
print $E         # 3x^3 + 6
$F = $E * $A  # Умножение
print $F      # (3x^3 + 6)(2x + 1) = 6x^4 + 3x^3 + 12x + 6
$G = $F ^ 2  # Степень (полином ^ NUMBER)
print $G     # Большой полином, но ок (степень <=1024)
$H = $G / $A  # Деление, вывести quotient и remainder
print $H      # Quotient: ..., Remainder: ...
$I = ($F)    # Производная
print $I      # 24x^3 + 9x^2 + 12
$J = - (x + 1)  # Унарный минус
print $J        # -x -1
# Комментарий: Тест с просто выражением (без присваивания)
print 4x^2 - 2(x + 3) + 1  # Все правила: опущенные *, унарный -, POW, скобки
# Пустая строка ок
print $J  # Производная от унарного